              }
            },
            "& when (@typeBlock = '1')": {
              ".fLink": {
                "@media (min-width: 992px)": {
                  "padding-left": "2rem",
                  "padding-right": "2rem",
                  "@media (max-width: 992px)": {
                    "padding-left": "0.5rem",
                    "padding-right": "0.5rem"
                  },
                  "&:not(:nth-last-child(1))": {
                    "border-right": "2px solid #efefef"
                  }
                }
              },
              ".mbr-row": {
                "margin": "0",
                "& when (@showCopyright)": {
                  "&:nth-child(1)": {
                    "margin-bottom": "1rem"
                  }
                }
              }
            },
            "& when (@typeBlock = '2')": {
              ".footer-container": {
                "& when (@showCopyright) and (@showItems)": {
                  "@media (min-width: 992px)": {
                    "justify-content": "space-between"
                  },
                  "@media (max-width: 991px)": {
                    "justify-content": "center"
                  }
                }
              },
              ".mbr-row": {
                "width": "auto",
                "@media (max-width: 992px)": {
                  "padding": "0",
                  "width": "100%",
                  "justify-content": "center",
                  "& when (@showCopyright)": {
                    "&:nth-child(1)": {
                      "margin-bottom": "1rem"
                    }
                  }
                }
              },
              ".copyright": {
                "width": "auto"
              }
            },
            "@media (max-width: 767px)": {
              ".link-items": {
                "-webkit-justify-content": "center",
                "justify-content": "center"
              }
            },
            ".copyright .mbr-text": {
              "color": "#143c46"
            }
